Xcursor*theme:neutral

urxvt*background:		gray25
urxvt*foreground:		lightgray
urxvt*cursorColor: 		lightgray
!urxvt*fading:   		10
!urxvt*tintColor: 		#000000
!urxvt*shading:   		25
urxvt*inheritPixmap: 		false
urxvt*scrollBar: 		false
urxvt*title: 			Terminal
urxvt*termName: 		rxvt-unicode
urxvt*scrollTtyKeypress:    	true
urxvt*scrollTtyOutput:      	false
urxvt*scrollWithBuffer:     	false
urxvt*perl-lib:			/usr/lib/urxvt/perl
urxvt*perl-ext-common:		default,tabbed,matcher
urxvt*urlLauncher:		/home/bonzodog/Documents/Downloaded/firefox-beta/firefox/firefox
urxvt*matcher.button:		1
urxvt*scrollstyle:      	plain
urxvt*secondaryScroll:  	true
urxvt*cutchars: 		`"'&()urxvt*,;<=>?@[]{|}$`
urxvt*font: 			xft:Pragmata:pixelsize=10:antialias=true:hinting=true
urxvt*boldFont: 		xft:Pragmata:pixelsize=10:antialias=true:hinting=true
urxvt*italicFont: 		xft:Pragmata:pixelsize=10:antialias=true:italic:autohint=true:hinting=true
urxvt*bolditalicFont: 		xft:Pragmata:pixelsize=10:antialias=true:italic:autohint=true:hinting=true
urxvt*iconName:			rxvt-unicode
urxvt*cursorBlink:        	true

